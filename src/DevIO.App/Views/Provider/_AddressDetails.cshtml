@model DevIO.App.ViewModels.ProviderViewModel

<div style="padding-top: 20px">
    <div>
        <hr />
        <h4>Endereço</h4>
    </div>

    @if (Model != null)
    {
        <table class="table table-hover">
            <thead class="thead-dark">
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.Address.PublicPlace)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Address.Number)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Address.AddressComplement)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Address.Neighborhood)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Address.ZipCode)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Address.City)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Address.State)
                    </th>
                    <th></th>
                </tr>
            </thead>

            <tr>
                <td>
                    @Html.DisplayFor(model => model.Address.PublicPlace)
                </td>
                <td>
                    @Html.DisplayFor(model => model.Address.Number)
                </td>
                <td>
                    @Html.DisplayFor(model => model.Address.AddressComplement)
                </td>
                <td>
                    @Html.DisplayFor(model => model.Address.Neighborhood)
                </td>
                <td>
                    @Html.DisplayFor(model => model.Address.ZipCode)
                </td>
                <td>
                    @Html.DisplayFor(model => model.Address.City)
                </td>
                <td>
                    @Html.DisplayFor(model => model.Address.State)
                </td>
                <td>
                    <a class="btn btn-info" data-modal="">
                        <span title="Editar" class="fa fa-pencil"></span>
                    </a>
                </td>
            </tr>
        </table>
    }

</div>

